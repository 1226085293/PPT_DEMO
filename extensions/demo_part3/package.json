{
	"package_version": 2,
	"name": "demo_part3",
	"version": "0.0.1",
	"description": "插件模板",
	"author": "_muzzik",
	"main": "./src/main.js",
	"scripts": {
		"build": "cc-plugin-cli c -p demo_part3"
	},
	"contributions": {
		"scene": {
			"script": "./src/scene.js"
		},
		"menu": [
			{
				"path": "i18n:menu.extension",
				"label": "i18n:demo_part3.title",
				"message": "open"
			}
		],
		"messages": {
			"open": {
				"methods": [
					"open"
				]
			}
		},
		"shortcuts": [
			{
				"message": "open",
				"win": "f1",
				"mac": "f1"
			}
		]
	},
	"panels": {
		"default": {
			"main": "./src/panel",
			"type": "dockable",
			"title": "mk插件模板",
			"size": {
				"width": 500,
				"height": 600,
				"min-width": 500,
				"min-height": 600
			}
		}
	},
	"devDependencies": {
		"@types/node": "^17.0.31",
		"@types/http-server": "^0.12.1"
	},
	"dependencies": {
		"element-plus": "^2.2.1",
		"http-server": "^14.1.1",
		"vue": "^3.2.33"
	}
}
